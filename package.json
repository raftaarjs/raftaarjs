{
  "name": "raftaarjs",
  "version": "0.0.1",
  "description": "light DOM to PWA compiler",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "serve": "es-dev-server --open",
    "build": "node ./",
    "build:watch": "watch \"npm run build\" ./src",
    "prepare": "pika install --optimize --clean --no-source-map --dest ./dist/web_modules/",
    "clean": "rm -rf node_modules/ dist/ .temp/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raftaarjs/raftaarjs.git"
  },
  "keywords": [
    "html",
    "css",
    "javascript",
    "pwa"
  ],
  "author": "Gaurav Panchal",
  "license": "GPL-3.0",
  "bin": {
    "raftaar": "./scripts/cli.js"
  },
  "bugs": {
    "url": "https://github.com/raftaarjs/raftaarjs/issues"
  },
  "homepage": "https://github.com/raftaarjs/raftaarjs#readme",
  "devDependencies": {
    "@pika/web": "^0.5.1",
    "concurrently": "^4.1.1",
    "es-dev-server": "^1.10.4",
    "ncp": "^2.0.0",
    "puppeteer": "^1.19.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@vaadin/router": "^1.2.1",
    "dimport": "^1.0.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.1"
  }
}
